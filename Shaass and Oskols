import sys
input = sys.stdin.readline

n = int(input())
birds = [0] + list(map(int, input().split()))
m = int(input())
for _ in range(m):
    x, y = map(int, input().split())
    if x > 1:
        birds[x - 1] += (y - 1)
    if x < n:
        birds[x + 1] += (birds[x] - y)
    birds[x] = 0
for i in range(1, n + 1):
    print(birds[i])
